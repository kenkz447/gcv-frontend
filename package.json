{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.@/ui/admin/uiConfig.js",
    "prepare": "husky install",
    "gen:api": ".generation/generate.sh"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.28.0",
    "@types/styled-jsx": "^3.4.4",
    "bootstrap": "^5.2.3",
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "next": "^13.2.4",
    "next-seo": "^5.15.0",
    "node-sass-middleware": "^1.0.1",
    "overlayscrollbars": "^2.1.0",
    "overlayscrollbars-react": "^0.5.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-fuzzy-highlighter": "^1.1.1",
    "react-icons": "^4.8.0",
    "react-intl": "^6.3.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "reduxjs-toolkit-persist": "^7.2.1",
    "styled-jsx": "^5.1.2",
    "styled-jsx-plugin-sass": "^1.0.0",
    "tailwind-merge": "^1.10.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@openapi-codegen/cli": "^2.0.0",
    "@openapi-codegen/typescript": "^6.1.2",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.59.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
